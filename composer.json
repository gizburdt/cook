{
    "name": "gizburdt/cook",
    "description": "Opinionated package to speed up new Laravel projects",
    "keywords": ["laravel", "dev"],
    "homepage": "https://github.com/gizburdt/cook",
    "license": "MIT",
    "authors": [
        {
            "name": "Gijs Jorissen",
            "homepage": "https://gijsjorissen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel-shift/cli": "^0.3.0",
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Gizburdt\\Cook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gizburdt\\Cook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gizburdt\\Cook\\CookServiceProvider"
            ]
        }
    }
}
