{
    "name": "gizburdt/cook",
    "description": "Opinionated package to speed up new Laravel projects",
    "keywords": ["laravel", "dev"],
    "homepage": "https://github.com/gizburdt/cook",
    "license": "MIT",
    "authors": [
        {
            "name": "Gijs Jorissen",
            "homepage": "https://gijsjorissen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nikic/php-parser": "^5.6"
    },
    "require-dev": {
        "illuminate/filesystem": "^11.0",
        "pestphp/pest": "^3.0",
        "symfony/process": "^7.0",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Gizburdt\\Cook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gizburdt\\Cook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gizburdt\\Cook\\CookServiceProvider"
            ]
        }
    }
}
